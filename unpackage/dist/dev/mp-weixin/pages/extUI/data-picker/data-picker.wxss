
view,
label,
swiper-item,
scroll-view {
  display:flex;
  flex-direction:column;
  flex-shrink: 0;
  flex-grow: 0;
  flex-basis: auto;
  align-items: stretch;
  align-content: flex-start;
}
view,
image,
input,
scroll-view,
swiper,
swiper-item,
text,
textarea,
video {
  position: relative;
  border: 0px solid #000000;
  box-sizing: border-box;
}
swiper-item {
  position: absolute;
}
button {
  margin: 0;
}


.container {
	height: 100%;

	display: flex;
	max-width: 500px;
	padding: 0 15px;

	flex-direction: column;
}
.title {
	font-size: 14px;
	font-weight: bold;
	margin: 20px 0 5px 0;
}
.data-pickerview {
	height: 400px;
	border: 1px #e5e5e5 solid;
}

